{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Glob(*)",
      "Grep(*)",
      "LS(*)",
      "Read(*)",
      "WebFetch(domain:*)",
      "Bash(afplay *)",
      "Bash(claude mcp *)",
      "Bash(gh issue list *)",
      "Bash(gh issue view *)",
      "Bash(gh pr checks *)",
      "Bash(gh pr list *)",
      "Bash(gh pr view *)",
      "Bash(gh repo view *)",
