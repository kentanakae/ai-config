      "Bash(gh repo clone *)",
      "Bash(gh repo fork *)",
      "Bash(git merge *)",
      "Bash(git push *)",
      "Bash(git rebase *)",
      "Bash(git restore *)",
      "Bash(mv *)",
      "Bash(npm install *)",
      "Bash(npx *)",
      "Bash(open *)",
      "Bash(rm *)",
      "Bash(rmdir *)",
      "Bash(wget *)"
    ]
  },
  "extraKnownMarketplaces": {
    "KNT-claude-code-plugins": {
      "source": {
        "source": "github",
        "repo": "kentanakae/claude-code-plugins"
      }
    }
  },
  "enabledPlugins": {
    "core@KNT-claude-code-plugins": true
  },
  "language": "japanese",
  "env": {
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "16000",
    "MAX_THINKING_TOKENS": "16000",
    "ENABLE_TOOL_SEARCH": "1",
    "DISABLE_TELEMETRY": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "CLAUDE_CODE_ADDITIONAL_DIRECTORIES_CLAUDE_MD": "1",
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1",
    "CLAUDE_CODE_DISABLE_AUTO_MEMORY": "0"
  },
  "model": "opus",
  "effortLevel": "high",
  "plansDirectory": ".claude/plans",
  "respectGitignore": false,
  "showTurnDuration": false,
  "fastMode": false,
  "outputStyle": "knt",
