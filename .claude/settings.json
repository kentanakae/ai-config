      "Bash(swift test *)",
      "Bash(xcrun *)",
      "Bash(xcodebuild *)",
      "Bash(git checkout *)",
      "Bash(git switch *)"
    ],
    "deny": [
      "Edit(**/.env*)",
      "Edit(**/.git/**)",
      "Edit(~/.aws/**)",
      "Edit(~/.ssh/**)",
      "Glob(//*)",
      "Read(**/.env*)",
      "Read(**/.git/**)",
      "Read(**/*.key)",
      "Read(**/*.pem)",
      "Read(**/client_secret*)",
      "Read(**/secrets.*)",
