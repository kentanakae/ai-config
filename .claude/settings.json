      "Bash(git *push --force *)",
      "Bash(git *push --force-with-lease)",
      "Bash(git *push --force-with-lease *)",
      "Bash(git *push -f)",
      "Bash(git *push -f *)",
      "Bash(git *reset --hard)",
      "Bash(git *reset --hard *)",
      "Bash(git clean -f *)",
      "Bash(git filter-branch *)",
      "Bash(git push --force *)",
      "Bash(git push --force-with-lease *)",
      "Bash(git push -f *)",
      "Bash(git reset --hard *)",
      "Bash(kill *)",
      "Bash(killall *)",
      "Bash(ls /)",
      "Bash(ls /etc/*)",
      "Bash(su)",
      "Bash(su *)",
      "Bash(sudo *)"
    ],
    "ask": [
      "Bash(brew install *)",
      "Bash(chmod *)",
      "Bash(colima *)",
      "Bash(curl *)",
      "Bash(docker *)",
      "Bash(gh api *)",
